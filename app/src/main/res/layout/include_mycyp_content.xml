<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="20dp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="76dp"
        android:layout_gravity="center"
        android:background="@color/white"
        android:orientation="horizontal">

        <RelativeLayout
            android:id="@+id/transaction_confirmation_rl"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/chengjiaoqueren_iv"
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="17dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/chengjiaoqueren_iv"
                android:layout_centerHorizontal="true"
                android:text="成交确认"
                android:textColor="@color/black3"
                android:textSize="12sp" />

            <!--android:layout_toRightOf="@+id/chengjiaoqueren_iv"-->
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/pending_payment_rl"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/daifukuan_iv"
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="17dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/daifukuan_iv"
                android:layout_centerHorizontal="true"
                android:text="待付款"
                android:textColor="@color/black3"
                android:textSize="12sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/in_delivery_rl"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/jiaofuzhong_iv"
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="17dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/jiaofuzhong_iv"
                android:layout_centerHorizontal="true"
                android:text="交付中"
                android:textColor="@color/black3"
                android:textSize="12sp" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/all_orders_rl"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/suoyoudingdan_iv"
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="17dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/suoyoudingdan_iv"
                android:layout_centerHorizontal="true"
                android:text="所有订单"
                android:textColor="@color/black3"
                android:textSize="12sp" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/historical_bidding_rl"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_gravity="center_vertical"
            android:background="@color/white"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:contentDescription="@string/app_name"
                android:padding="5dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:id="@+id/tv02_cus"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="55dp"
                android:drawablePadding="4dp"
                android:gravity="center"
                android:text="历史竞价"
                android:textColor="@color/black3"
                android:textSize="15sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="44dp"
                android:gravity="center"
                android:text="与我擦肩而过的车辆"
                android:textSize="12sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="13dp"
                android:contentDescription="@string/app_name"
                android:paddingBottom="15dp"
                android:paddingTop="15dp"
                android:src="@mipmap/jindutixin_icon" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_mycyp_zichan"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@color/white">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:contentDescription="@string/app_name"
                android:padding="5dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="55dp"
                android:drawablePadding="4dp"
                android:gravity="center"
                android:text="我的钱包"
                android:textColor="@color/black3"
                android:textSize="15sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="13dp"
                android:contentDescription="@string/app_name"
                android:paddingBottom="15dp"
                android:paddingTop="15dp"
                android:src="@mipmap/jindutixin_icon" />
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/mycyp_help_center"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@color/white">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:contentDescription="@string/app_name"
                android:padding="5dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="55dp"
                android:drawablePadding="4dp"
                android:gravity="center"
                android:text="帮助中心"
                android:textColor="@color/black3"
                android:textSize="15sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="13dp"
                android:contentDescription="@string/app_name"
                android:paddingBottom="15dp"
                android:paddingTop="15dp"
                android:src="@mipmap/jindutixin_icon" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlCustomerService"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@color/white">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:contentDescription="@string/app_name"
                android:padding="5dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="55dp"
                android:drawablePadding="4dp"
                android:gravity="center"
                android:text="客服中心"
                android:textColor="@color/black3"
                android:textSize="15sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="13dp"
                android:contentDescription="@string/app_name"
                android:paddingBottom="15dp"
                android:paddingTop="15dp"
                android:src="@mipmap/jindutixin_icon" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlMyCounselor"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@color/white"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:contentDescription="@string/app_name"
                android:padding="5dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:id="@+id/tv_myguwen"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="55dp"
                android:drawablePadding="4dp"
                android:gravity="center"
                android:text="我的顾问"
                android:textColor="@color/black3"
                android:textSize="15sp" />

            <ImageView
                android:id="@+id/tv_myguwen_point"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dip"
                android:layout_toRightOf="@id/tv_myguwen"
                android:contentDescription="@string/app_name"
                android:src="@mipmap/jindutixin_icon" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="13dp"
                android:contentDescription="@string/app_name"
                android:paddingBottom="15dp"
                android:paddingTop="15dp"
                android:src="@mipmap/jindutixin_icon" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_advise_feedback"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@color/white"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:contentDescription="@string/app_name"
                android:padding="5dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:id="@+id/tv_help_feedback"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="55dp"
                android:drawablePadding="4dp"
                android:gravity="center"
                android:text="建议反馈"
                android:textColor="@color/black3"
                android:textSize="15sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="13dp"
                android:contentDescription="@string/app_name"
                android:paddingBottom="15dp"
                android:paddingTop="15dp"
                android:src="@mipmap/jindutixin_icon" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlSetting"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@color/white">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:contentDescription="@string/app_name"
                android:padding="5dp"
                android:src="@mipmap/jindutixin_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="55dp"
                android:drawablePadding="4dp"
                android:gravity="center"
                android:text="设置"
                android:textColor="@color/black3"
                android:textSize="15sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="13dp"
                android:contentDescription="@string/app_name"
                android:paddingBottom="15dp"
                android:paddingTop="15dp"
                android:src="@mipmap/jindutixin_icon" />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>